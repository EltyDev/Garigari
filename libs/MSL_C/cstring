#ifndef CSTRING_H
#define CSTRING_H

#include "size_t.h"

#ifdef __cplusplus
extern "C" {
#endif

size_t strlen(const char *);
int strcmp(const char *, const char *);

char* strchr(const char *, int);
char* strrchr(const char *, int);
char* strstr(const char *, const char *);

#ifdef __cplusplus
}
#endif

#endif // CSTRING_H
